{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/assets/*",
      "/*.css",
      "/*.js",
      "/*.png",
      "/*.jpg",
      "/*.svg",
      "/*.ico",
      "/wasm/*",
      "/model.json",
      "/group*-shard*"
    ]
  },
  "globalHeaders": {
    "Permissions-Policy": "camera=(self), microphone=(self), autoplay=(self)"
  }
}
