{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/assets/*",
      "/*.png",
      "/*.jpg",
      "/*.jpeg",
      "/*.gif",
      "/*.svg",
      "/*.ico",
      "/manifest.webmanifest",
      "/robots.txt"
    ]
  },
  "globalHeaders": {
    "Permissions-Policy": "camera=(self), microphone=(), autoplay=(self)"
  }
}
